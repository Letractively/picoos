<?xml version="1.0" encoding="UTF-8">
<roles>
	<role name="Administrator" desc="Access to all functionality">
		<permissions>
			<permission object="*" action="*"/>
		</permissions>
	</role>
	<role name="Viewer" desc="Access to all functionality read only">
		<permissions>
			<permission object="*" action="read"/>
		</permissions>
	</role>
l	<role name="ReportViewer" desc="Access to all report functionality">
		<permissions>
			<permission object="Report" action="*"/>
		</permissions>
	</role>
</roles>